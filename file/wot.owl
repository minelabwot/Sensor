    <label>observedBy</label>
    <seeAlso>http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton</seeAlso>
  </owl:ObjectProperty>
  <!-- http://purl.oclc.org/NET/ssnx/ssn#observedProperty -->
